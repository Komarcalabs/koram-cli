{
  "name": "koram",
  "description": "El Toolkit sagrado del buen Komarquino",
  "version": "1.0.3",
  "author": "Dick Van Ocampo",
  "bin": {
    "koram": "./bin/run"
  },
  "bugs": "https://github.com/Komarcalabs/koram-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "detect-port": "^2.1.0",
    "express": "^4.17.1",
    "glob": "^11.0.3",
    "inquirer": "^8.2.7",
    "keytar": "^7.9.0",
    "marked": "^16.1.2",
    "node-ssh": "^13.2.1",
    "open": "^7.3.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "koram",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/Komarcalabs/koram-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "python3 -m venv venv && venv/bin/pip install -r src/python-deployer/requirements.txt"
  }
}
